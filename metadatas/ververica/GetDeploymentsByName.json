{
  "name": "GetDeploymentsByName",
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/deployments/name/[deploymentName]",
  "parameters": [
    {
      "name": "deploymentName",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "ignoreJobSummary",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "ignoreResourceSetting",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "namespace",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}