  "parameters": [
    {
      "name": "Description",
      "description": "数据源的备注信息，不能超过 128 个字符。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "DgInstanceId",
      "description": "DG 网关 ID。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "InstanceId",
      "description": "数据库实例 ID。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "Ip",
      "description": "数据库宿主机的 IP。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "Name",
      "description": "数据源的名称。",
      "position": "Body",
      "type": "String",
      "required": true
    },
    {
      "name": "Password",
      "description": "数据库用户的密码，需要使用 Base64 加密。",
