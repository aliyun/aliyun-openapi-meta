    },
    {
      "name": "projectName",
      "description": "MaxCompute项目名称",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "sourceProject",
      "description": "package所属项目。如果package 是install 的，此参数必填。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}