{
  "name": "ListFunctions",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v1/projects/[projectName]/functions",
  "parameters": [
    {
      "name": "marker",
      "description": "设定结果从marker之后按字母排序的第一个开始返回。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "maxItem",
      "description": "每页返回的最大结果数。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "prefix",
      "description": "限定返回的资源名称，名称必须以Prefix作为前缀，例如Prefix是a，则返回的资源名均是以a开始的。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "projectName",
      "description": "项目名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "schemaName",
      "description": "schema名称。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}