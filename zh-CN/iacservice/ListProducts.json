{
  "name": "ListProducts",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/products",
  "parameters": [
    {
      "name": "keyword",
      "description": "搜索产品关键词支持模糊查询",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "maxResults",
      "description": "每页的最大数量，取值范围（0-200），默认100",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "nextToken",
      "description": "分页nextToken",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "sort",
      "description": "排序方式\n- Normal：默认排序（默认值）\n- Top：热门访问排序",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "status",
      "description": "状态，Available，Deprecated",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "supportTerraformer",
      "description": "是否支持Terraformer",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "terraformProviderVersion",
      "description": "TerraformProvider版本，为空时默认为最新版本",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}