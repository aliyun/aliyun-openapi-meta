{
  "name": "ListTasks",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/tasks",
  "parameters": [
    {
      "name": "groupId",
      "description": "分组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "keyword",
      "description": "关键字，按照任务ID或任务名称模糊搜索",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "moduleId",
      "description": "模板ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "页码，默认1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "每页返回的结果数量。默认值：20，最小值：1，最大值：100。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "projectId",
      "description": "项目ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "status",
      "description": "作业状态，取值\n\n- Planning：作业正处于执行Plan阶段\n- Planned：作业完成了Plan的执行\n- PlannedAndFinished：执行完Plan之后，发现没有任何diff，此时作业处于最终状态\n- Applying：作业正处于执行Apply阶段\n- Applied：作业完成Apply的执行\n- Errored：作业的执行有错误，进入最终状态",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "tag",
      "description": "任务标签列表。",
      "position": "Query",
      "type": "Json",
      "required": false,
      "sub_parameters": [
        {
          "name": "tagKey",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "tagValue",
          "position": "Query",
          "type": "String",
          "required": false
        }
      ]
    },
    {
      "name": "taskId",
      "description": "任务ID",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}