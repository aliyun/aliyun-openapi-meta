{
  "name": "OperateJob",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "/tasks/[taskId]/jobs/[jobId]/operation/[operationType]",
  "parameters": [
    {
      "name": "comment",
      "description": "描述，长度（0-128）",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "jobId",
      "description": "作业ID",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "operationType",
      "description": "操作类型，\n取值范围：\n- execute 执行\n- abolish 放弃\n- cancel 取消",
      "position": "Path",
      "type": "String",
      "required": false
    },
    {
      "name": "taskId",
      "description": "任务ID",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "taskType",
      "description": "任务类型\n\nTask：常规任务（默认）\n\nSceneTestingTask：场景化测试任务",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}