{
  "name": "PublishEdgeContainerAppVersion",
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "AppId",
      "description": "应用ID。",
      "position": "Body",
      "type": "String",
      "required": true
    },
    {
      "name": "FullRelease",
      "description": "是否全量发布。该字段仅当 PublishType设置为 region 时生效。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "Percentage",
      "description": "发布比例。取值：**1~100**，默认值：**100**。",
      "position": "Body",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PublishEnv",
      "description": "发布的环境，取值如下：\n\n- **prod**: 生产环境。\n- **staging**：预发环境。",
      "position": "Body",
      "type": "String",
      "required": true
    },
    {
      "name": "PublishType",
      "description": "发布类型，支持如下两种：\n- **percentage**：按照百分比发布。\n\n- **region**：按照区域发布。\n\n若不填写，默认使用百分比发布。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Regions",
      "description": "发布区域列表。",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "Remarks",
      "description": "备注信息，默认为空。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "StartTime",
      "description": "开始进行发布的时间，不写则默认为当前时间。\n\n日期格式按照ISO8601表示法，并使用UTC+0时间，格式为yyyy-MM-ddTHH:mm:ssZ。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "VersionId",
      "description": "版本ID。",
      "position": "Query",
      "type": "String",
      "required": true
    }
  ]
}