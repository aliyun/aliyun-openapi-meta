{
  "name": "InstallManagedPrometheus",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "ClusterId",
      "description": "阿里云容器服务Kubernetes版的Kubernetes集群的ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ClusterName",
      "description": "集群名称。ClusterType为ecs时，必填。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ClusterType",
      "description": "集群类型。目前只支持ask、ecs和one集群。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "GrafanaInstanceId",
      "description": "集群绑定的托管版Grafana工作区Id。为空或\"free\"时，绑定到共享版Grafana。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "KubeConfig",
      "description": "当前参数不支持使用。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "地域ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "Prometheus实例的资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SecurityGroupId",
      "description": "集群所对应的安全组。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "VSwitchId",
      "description": "集群所对应的交换机。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "VpcId",
      "description": "集群所对应的VPC。",
      "position": "Query",
      "type": "String",
      "required": true
    }
  ]
}