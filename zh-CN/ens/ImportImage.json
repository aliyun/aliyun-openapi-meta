{
  "name": "ImportImage",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "Architecture",
      "description": "系统架构。取值范围：</br>\n\n- x86_64。\n\n现只支持：x86_64。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ComputeType",
      "description": "`镜像类型`\nens_vm：ens虚机镜像（默认）",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "DiskDeviceMapping",
      "description": "创建的自定义镜像信息列表。",
      "position": "Query",
      "type": "Json",
      "required": false,
      "sub_parameters": [
        {
          "name": "OSSBucket",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "OSSObject",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "OSSRegion",
          "position": "Query",
          "type": "String",
          "required": false
        }
      ]
    },
    {
      "name": "ImageFormat",
      "description": "镜像格式。取值范围：</br>\nqcow2。</br>\n当前只支持qcow2。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ImageName",
      "description": "镜像名称。长度为[2, 128]个英文或中文字符。必须以大小字母或中文开头，不能以http://和https://开头。可以包含数字、半角冒号（:）、下划线（_）或者连字符（-）。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "LicenseType",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OSSBucket",
      "description": "镜像文件所在的OSS Bucket。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OSSObject",
      "description": "镜像文件名。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OSSRegion",
      "description": "镜像所在Region，暂时只支持cn-beijing",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OSType",
      "description": "操作系统平台类型。取值范围：\n\n- windows。\n- linux。\n\n暂时只支持：linux。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "OSVersion",
      "description": "操作系统分发版版本",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Platform",
      "description": "操作系统发行版。取值范围：\n* centos\n* ubuntu",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TargetOSSRegionId",
      "description": "镜像要存储的目标OSS的区域。</br>\n\n> 当前只支持cn-beijing与ap-southeast-1两个区域。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}