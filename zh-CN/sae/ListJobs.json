{
  "name": "ListJobs",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/pop/v1/sam/job/listJobs",
  "parameters": [
    {
      "name": "AppName",
      "description": "任务模板名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "CurrentPage",
      "description": "当前页数。取值从1开始。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "FieldType",
      "description": "设置筛选任务模板的维度。取值说明如下：\n\n- **appName**：任务模板名称。\n- **appIds**：任务模板ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "FieldValue",
      "description": "根据已设置的**FieldType**筛选维度，选择输入目标任务模板的任务模板名称或任务模板ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "NamespaceId",
      "description": "命名空间ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OrderBy",
      "description": "对任务模板进行排序。取值说明如下：\n\n- **running**：按照当前实例数进行排序。\n- **instances**：按照目标实例数进行排序。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "页面大小。取值范围为\\[0,200]。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Reverse",
      "description": "根据**OrderBy**参数传入的字段名排序。取值说明如下：\n\n  - **true**：按升序排序。\n  - **false**：按降序排序。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "Tags",
      "description": "标签列表，JSON字符串。取值说明如下：\n\n- **key**：标签键。\n- **value**：标签值。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Workload",
      "description": "Workload需指定为`job`。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}