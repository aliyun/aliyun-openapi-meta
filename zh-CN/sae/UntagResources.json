{
  "name": "UntagResources",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "DELETE",
  "pathPattern": "/tags",
  "parameters": [
    {
      "name": "DeleteAll",
      "description": "是否删除所有标签，当已传入标签键时生效。取值说明如下：\n\n- **true**：删除所有标签。\n- **false**：不删除所有标签。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "地域ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ResourceIds",
      "description": "资源ID。可填写多个资源ID，用英文逗号（,）分隔。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ResourceType",
      "description": "资源类型，只支持`application`。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "TagKeys",
      "description": "标签键列表。可填写多个标签键，用英文逗号（,）分隔。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}