{
  "name": "ListTrainingJobMetrics",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v1/trainingjobs/[TrainingJobId]/metrics",
  "parameters": [
    {
      "name": "EndTime",
      "description": "截止UTC时间，格式 iso8601，假如为空是当前时间",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Name",
      "description": "指标名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Order",
      "description": "指标返回的时间顺序，ASC或者DESC。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "页码。",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页的大小。",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "StartTime",
      "description": "起始UTC时间，格式 iso8601，假如为空是任务开始时间",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TrainingJobId",
      "description": "训练任务ID。",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}