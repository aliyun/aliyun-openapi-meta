{
  "name": "GetNodeMetrics",
  "deprecated": true,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v1/resources/[ResourceGroupID]/nodemetrics/[MetricType]",
  "parameters": [
    {
      "name": "EndTime",
      "description": "结束时间",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "GPUType",
      "description": "显卡类型。可能值：\n- V100\n- A100\n- A10\n- T4\n- P100",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "MetricType",
      "description": "查询的指标名",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "ResourceGroupID",
      "description": "资源组 ID，每一个资源组都有全局唯一的资源组ID。可以通过资源组ID来获取资源组的信息。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "StartTime",
      "description": "开始时间",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TimeStep",
      "description": "时间步长，默认值：5m。支持的时长单位的取值范围：\n\n- h：小时。\n\n- m：分钟。\n\n- s：秒。\n\n当值不带单位时，默认使用单位：s（秒）。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Verbose",
      "description": "是否显示非必要信息，非必要信息目前包含Labels\n目前值支持：\n\nfalse：不显示非必要信息（默认值）\ntrue：显示非必要信息",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}