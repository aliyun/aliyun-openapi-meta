{
  "name": "ListNodes",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v1/nodes",
  "parameters": [
    {
      "name": "AcceleratorType",
      "description": "加速器类型，枚举：\n\n- CPU\n- GPU\n\n默认为空，代表所有类型。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "AvailabilityZone",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "CliqueID",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "FilterByQuotaId",
      "description": "与ResourceGroupIds同时使用，展示资源组里面Quota绑定某些Quota的node。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "FilterByResourceGroupIds",
      "description": "与QuotaId同时使用，展示Quota中属于某些资源组的node。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "GPUType",
      "description": "按卡类型，支持模糊匹配。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "HealthCount",
      "description": "健康的节点数",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "HealthRate",
      "description": "节点的健康率（健康节点的占比）",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "HyperNode",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "HyperZone",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "LayoutMode",
      "description": "布局模式",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "MachineGroupIds",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "NodeNames",
      "description": "只显示指定节点，按逗号分隔。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "NodeStatuses",
      "description": "节点所处状态。按逗号分隔，不设置会返回所有状态的节点列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "NodeTypes",
      "description": "资源节点规格列表，按逗号分隔，不设置会返回所有资源节点规格的机器列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Order",
      "description": "排列顺序，取值：\n- desc：降序。\n- asc：升序。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OrderInstanceIds",
      "description": "订单Id信息",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "OrderStatuses",
      "description": "节点所在订单状态。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "分页页码，从1开始。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页查询每页数据集数量。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PaymentType",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "QuotaId",
      "description": "节点所属资源配额ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ReasonCodes",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupIds",
      "description": "指定资源组ID列表，按逗号分隔，和QuotaId参数二选一必填\n约束：\n1. 需校验传入的ResourceGroupId的UserId和请求的UserId一致\n2. 传入的ResourceGroup列表类型需一致\n3. 传入的ResourceGroup列表现阶段记录的VPC需一致",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SortBy",
      "description": "排序依据。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Verbose",
      "description": "是否返回资源使用信息。只在QuotaId设置时生效。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "WorkspaceId",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}