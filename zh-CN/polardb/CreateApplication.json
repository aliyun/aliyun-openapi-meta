{
  "name": "CreateApplication",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "ApplicationType",
      "description": "应用类型，取值范围为\n\n- supabase: 若创建托管 Supabase 应用，请传入此值\n- raycluster: 若创建托管 Ray Cluster 应用，请传入此值",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Architecture",
      "description": "CPU 架构，取值范围\n\n- x86",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "AutoCreatePolarFs",
      "description": "是否自动创建冷存版PolarFs，取值：\n* false（默认）：不自动创建。\n* true：自动创建。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "AutoRenew",
      "description": "自动续费",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "AutoUseCoupon",
      "description": "是否自动使用优惠券，取值：\n* true（默认）：使用优惠券。\n* false：不使用优惠券。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "Components",
      "description": "用户自定义的应用子组件列表",
      "position": "Query",
      "type": "Json",
      "required": false,
      "sub_parameters": [
        {
          "name": "ComponentClass",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "ComponentMaxReplica",
          "position": "Query",
          "type": "Long",
          "required": false
        },
        {
          "name": "ComponentReplica",
          "position": "Query",
          "type": "Long",
          "required": false
        },
        {
          "name": "ComponentType",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "ScaleMax",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "ScaleMin",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "SecurityGroups",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "SecurityIPArrayName",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "SecurityIPList",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "SecurityIPType",
          "position": "Query",
          "type": "String",
          "required": false
        }
      ]
    },
    {
      "name": "DBClusterId",
      "description": "应用依赖的PolarDB实例ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Description",
      "description": "应用描述/备注",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DryRun",
      "description": "默认为`false`，设置为`true`后将只进行参数和资源校验，不实际创建资源。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "Endpoints",
      "description": "用户自定义的服务端点列表，默认创建一个VPC Endpoint",
      "position": "Query",
      "type": "Json",
      "required": false,
      "sub_parameters": [
        {
          "name": "Description",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "EndpointType",
          "position": "Query",
          "type": "String",
          "required": false
        }
      ]
    },
    {
      "name": "PayType",
      "description": "付费类型",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Period",
      "description": "包年或包月类型",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PolarFSInstanceId",
      "description": "PolarFS 冷存版/高性能版实例ID，默认为空，若传入则将相应存储挂载到应用中\n\n目前仅如下应用支持\n- supabase\n- raycluster",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PromotionCode",
      "description": "优惠券编号，不填时使用默认优惠券",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "地域，默认为实例地域",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "资源组ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "UsedTime",
      "description": "期限",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "VSwitchId",
      "description": "交换机，默认为实例主可用区当前的交换机",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "VpcId",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ZoneId",
      "description": "可用区，默认为实例主可用区",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}