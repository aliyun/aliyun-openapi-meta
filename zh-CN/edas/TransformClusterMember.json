{
  "name": "TransformClusterMember",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "/pop/v5/resource/transform_cluster_member",
  "parameters": [
    {
      "name": "InstanceIds",
      "description": "实例ID集合 ，用半角逗号（,）分割：\n- 实例可以不属于任何集群，但一个实例最多只能归属于一个集群。\n- ECS实例和目标集群需在同一个VPC内。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Password",
      "description": "加入目标集群的实例需要设置的登录密码。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "TargetClusterId",
      "description": "目标集群ID。",
      "position": "Query",
      "type": "String",
      "required": true
    }
  ]
}