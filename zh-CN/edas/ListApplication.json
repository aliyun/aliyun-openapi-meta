{
  "name": "ListApplication",
  "deprecated": false,
  "security": [
    "AK"
  ],
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "/pop/v5/app/app_list",
  "parameters": [
    {
      "name": "AppIds",
      "description": "应用ID列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "AppName",
      "description": "以appName维度筛选应用列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ClusterId",
      "description": "以集群维度筛选应用列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "CurrentPage",
      "description": "分页查询，当前第几页，默认值为1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "LogicalRegionId",
      "description": "以微服务空间维度匹配筛选应用列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "LogicalRegionIdFilter",
      "description": "按精确匹配微服务空间的方式筛选应用。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页查询，每页显示的条数。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "以资源组维度筛选应用列表。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}