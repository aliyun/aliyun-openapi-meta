{
  "name": "ExecContainerCommand",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "Command",
      "description": "要在容器内执行的命令序列。最多20个命令，单个命令最长256个字符。    \n执行多个命令时，字符串需按照JSON格式传入，例如：`[\"/bin/sh\", \"-c\", \"ls -a\"]`。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ContainerGroupId",
      "description": "实例ID，即容器组ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ContainerName",
      "description": "容器名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "RegionId",
      "description": "实例所属的地域ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Stdin",
      "description": "是否标准输入。默认为true。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "Sync",
      "description": "是否立即执行命令并同步返回结果。默认为false。    \n如果设置为true，TTY只能设置为false。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "TTY",
      "description": "是否开启交互。默认为false。    \n当Command为/bin/bash命令时，需要设置为true。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}