{
  "name": "CreateActionPlan",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "ActionPlanName",
      "description": "执行计划的名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "AllocationSpec",
      "description": "资源类型。\n- Standard：标准型。\n- Dedicated：专属型，需开通白名单使用。\n- Economic：经济型，需开通白名单使用。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "AppId",
      "description": "应用ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DesiredCapacity",
      "description": "执行计划期望的资源规模。如果ResourceType是VcpuCapacity的话，该字段表示执行计划希望保有10000vCPU。",
      "position": "Query",
      "type": "Double",
      "required": false
    },
    {
      "name": "IntervalMinutes",
      "description": "执行计划的定时周期。单位分钟，取值范围：10 ～ 10080，默认值60。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Level",
