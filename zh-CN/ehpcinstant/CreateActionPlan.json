      "position": "Query",
      "type": "Json",
      "required": false,
      "sub_parameters": [
        {
          "name": "RegionId",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "SecurityGroupId",
          "position": "Query",
          "type": "RepeatList",
          "required": false
        },
        {
          "name": "SecurityGroupIds",
          "position": "Query",
          "type": "RepeatList",
          "required": false
        },
        {
