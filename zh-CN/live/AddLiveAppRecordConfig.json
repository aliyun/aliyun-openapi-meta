{
  "name": "AddLiveAppRecordConfig",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "AppName",
      "description": "播流所属应用名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "DelayTime",
      "description": "断流拼接时长。\n直播断流时长超过设定的拼接时长后，将会生成新文件，断流拼接时长支持15~21600秒。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
