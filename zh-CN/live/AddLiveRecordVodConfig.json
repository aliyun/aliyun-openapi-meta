      "type": "String",
      "required": false
    },
    {
      "name": "StreamName",
      "description": "播流名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "VodTranscodeGroupId",
      "description": "点播转码模板组ID。",
      "position": "Query",
      "type": "String",
      "required": true
    }
  ]
