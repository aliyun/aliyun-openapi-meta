{
  "name": "AddLiveStreamWatermark",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "Description",
      "description": "自定义水印描述信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Domain",
      "description": "播流域名。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Height",
      "description": "水印图片高度，单位：px。对比参考背景RefHeight，最终按照⽐例进⾏⾃适应缩放为准。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "Name",
      "description": "水印名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "OffsetCorner",
      "description": "位置。取值：\n\n- TopLeft：左上。\n\n- TopRight：右上。\n\n- BottomLeft：左下。\n\n- BottomRight：右下。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "PictureUrl",
      "description": "水印图片链接。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "RefHeight",
      "description": "参考背景视频高，单位：px。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "RefWidth",
      "description": "参考背景视频宽，单位：px。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "RegionId",
      "description": "地域ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Transparency",
      "description": "水印透明度，数值越小越透明。取值：0~255。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "Type",
      "description": "水印类型。取值：\n\n- **0**：图片。\n\n- **1**：文字（目前只支持图片）。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "XOffset",
      "description": "⽔印X轴偏移，单位：px。\n>相对于参考背景宽RefWidth， 如果选择的是TopLeft，那么XOffset是指⽔印图⽚的左上⻆和背景视频的左上⻆的相对位置X。其中坐标指向背景视频中⼼的⽅向为正⽅向，即X轴的正⽅向是向右。",
      "position": "Query",
      "type": "Float",
      "required": true
    },
    {
      "name": "YOffset",
      "description": "⽔印Y轴偏移，单位：px。\n>相对于参考背景⾼RefHeight，如果选择的是TopLeft，那么YOffset是指⽔印图⽚的左上⻆和背景视频的左上⻆的相对位置Y。其中坐标指向背景视频中⼼的⽅向为正⽅向，即Y轴的正⽅向是向下。",
      "position": "Query",
      "type": "Float",
      "required": true
    }
  ]
}