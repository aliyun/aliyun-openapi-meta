{
  "name": "AddTrancodeSEI",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "AppName",
      "description": "播流所属应用名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Delay",
      "description": "接收到命令后的延迟时间。单位： 毫秒。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "DomainName",
      "description": "播流域名。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Pattern",
      "description": "追加到每一个关键帧或每一帧。取值：\n- **keyframe**：关键帧。\n- **frame**：每一帧。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Repeat",
      "description": "重复次数。-1代表无限。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "StreamName",
      "description": "播流名称。\n\n> 必须为源流名称，所有的转码流都会添加上SEI信息。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Text",
      "description": "SEI文本。长度限制：4000字节。",
      "position": "Query",
      "type": "String",
      "required": true
    }
  ]
}