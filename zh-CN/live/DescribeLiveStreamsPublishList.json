{
  "name": "DescribeLiveStreamsPublishList",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "AppName",
      "description": "播流所属应用名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DomainName",
      "description": "推流域名或主播流域名。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "EndTime",
      "description": "结束时间。EndTime和StartTime之间的间隔不能超过30天。\n\n格式为：<i>yyyy-MM-dd</i>T<i>HH:mm:ss</i>Z（UTC时间）。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "OrderBy",
      "description": "排序方式。取值：\n\n- **stream_name_desc**：按直播流名称倒序。\n- **stream_name_asc**：按直播流名称升序。\n- **publish_time_desc**：按推流时间倒序。\n- **publish_time_asc**（默认值）：按推流时间升序。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "当前页码。默认值为**1**。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页大小。取值范围：**1~3000**，默认值为**2000**。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "QueryType",
      "description": "指定是否模糊匹配流名称。取值：\n- **fuzzy**（默认值）：模糊匹配。\n- **strict**：精准匹配。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StartTime",
      "description": "推流时间。\n\n格式为：<i>yyyy-MM-dd</i>T<i>HH:mm:ss</i>Z（UTC时间）。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "StreamName",
      "description": "播流名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StreamType",
      "description": "流类型。取值：\n\n- **all**（默认值）：查询所有流。\n- **raw**：查询原始流。\n- **trans**：查询转码流。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}