{
  "name": "ListDatasets",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "CreatorId",
      "description": "创建者ID",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "DataTypeList",
      "description": "数据类型，可多选，当前支持\n- COMMON 通用\n- PIC 图片\n- TEXT 文本\n- TABLE 表格\n- VIDEO 视频\n- AUDIO 音频\n- INDEX 索引",
      "position": "Body",
      "type": "RepeatList",
      "required": false
    },
    {
      "name": "Name",
      "description": "数据集名称，支持模糊搜索",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "Order",
      "description": "排序方向，默认为Desc（降序）",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "Origin",
      "description": "数据集来源，取值有\n- DataWorks\n- PAI",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "分页页码，默认为1",
      "position": "Body",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页大小，默认为10，最大100",
      "position": "Body",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ProjectId",
      "description": "DataWorks工作空间ID",
      "position": "Body",
      "type": "Long",
      "required": false
    },
    {
      "name": "SortBy",
      "description": "排序字段，默认为 CreateTime(创建时间)",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "StorageTypeList",
      "description": "存储类型，可多选，当前支持\n- OSS  对象存储\n- NAS  通用型NAS文件存储\n- EXTREMENAS  极速型NAS文件存储\n- DLF_LANCE  数据湖构建\n- CPFS  CPFS文件存储\n- BMCPFS 智算CPFS文件存储\n- MAXCOMPUTE  MaxCompute表",
      "position": "Body",
      "type": "RepeatList",
      "required": false
    }
  ]
}