{
  "name": "UntagResources",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "DELETE",
  "pathPattern": "/tags",
  "parameters": [
    {
      "name": "all",
      "description": "是否删除全部自定义标签，仅当`tag_keys`为空时生效。取值范围：\n\n- `true`: 删除全部标签。\n- `false`: 不删除全部标签。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "region_id",
      "description": "资源所属的地域ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "resource_ids",
      "description": "资源ID列表。",
      "position": "Query",
      "type": "Json",
      "required": true
    },
    {
      "name": "resource_type",
      "description": "资源类型定义。取值范围：只支持`CLUSTER`这一种资源类型。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "tag_keys",
      "description": "资源标签键列表。",
      "position": "Query",
      "type": "Json",
      "required": true
    }
  ]
}