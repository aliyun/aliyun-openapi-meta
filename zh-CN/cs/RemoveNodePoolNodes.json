{
  "name": "RemoveNodePoolNodes",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "DELETE",
  "pathPattern": "/clusters/[ClusterId]/nodepools/[NodepoolId]/nodes",
  "parameters": [
    {
      "name": "ClusterId",
      "description": "集群ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "NodepoolId",
      "description": "节点池ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "concurrency",
      "description": "是否并发移除。\n\n- true：节点将并发从伸缩组中被移除。\n\n- false：节点将顺序从伸缩组中被移除。\n\n默认值：false。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "drain_node",
      "description": "是否排水节点。可选值：\n- true：排水。\n- false：不排水。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "instance_ids",
      "description": "待移除的实例列表。",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "nodes",
      "description": "【该参数已废弃】\n\n移除节点列表。\n\n><danger>该参数已废弃，请使用`instance_ids`代替。</danger>",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "release_node",
      "description": "是否释放节点。可选值：\n- true：释放。\n- false：不释放。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}