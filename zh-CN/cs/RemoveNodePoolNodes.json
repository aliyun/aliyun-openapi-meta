{
  "name": "RemoveNodePoolNodes",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "DELETE",
  "pathPattern": "/clusters/[ClusterId]/nodepools/[NodepoolId]/nodes",
  "parameters": [
    {
      "name": "ClusterId",
      "description": "集群ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "NodepoolId",
      "description": "节点池ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "concurrency",
      "description": "是否并发移除。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "drain_node",
      "description": "是否排水节点。可选值：\n- true：排水。\n- false：不排水。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "instance_ids",
      "description": "移除实例列表。",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "nodes",
      "description": "移除节点列表。",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "release_node",
      "description": "是否释放节点。可选值：\n- true：释放。\n- false：不释放。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}