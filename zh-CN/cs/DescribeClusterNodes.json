{
  "name": "DescribeClusterNodes",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/clusters/[ClusterId]/nodes",
  "parameters": [
    {
      "name": "ClusterId",
      "description": "集群ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "instanceIds",
      "description": "节点实例ID列表，多值使用英文逗号（,）分隔。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "nodepool_id",
      "description": "节点池ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "页码。\n\n默认值：1。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "每页显示的记录数。取值范围为[1,100]。\n\n默认值：10。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "state",
      "description": "集群节点状态，按照集群节点运行状态进行过滤，取值：\n\n- `all`：以下4种状态的集群节点。\n- `running`：正在运行的集群节点。\n- `removing`：正在删除的集群节点。\n- `initial`：正在初始化的集群节点。\n- `failed`：创建失败的集群节点。\n\n默认值：`all`。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}