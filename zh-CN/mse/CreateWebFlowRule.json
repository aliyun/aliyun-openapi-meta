{
  "name": "CreateWebFlowRule",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "AcceptLanguage",
      "description": "返回结果显示的语言。取值：zh（默认值）：中文，en：英文",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "AppId",
      "description": "应用Id",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "AppName",
      "description": "应用名",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Burst",
      "description": "针对突发请求额外允许的请求数目（效果等同于令牌桶算法的最大令牌数）",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ControlBehavior",
      "description": "流控方式\n- 0(快速失败)\n- 2(排队等待)",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Enable",
      "description": "规则状态\n- true(开启)\n- false(关闭)",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "MaxQueueingTimeMs",
      "description": "超时时间，流控效果为排队等待时需要设置，单位毫秒。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "MetricType",
      "description": "阈值类型\n- 1(请求数)",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Namespace",
      "description": "命名空间",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ParamItem",
      "description": "参数描述项，JSON格式的字符串，包含以下键值对\n- parseStrategy：参数属性（0: Client IP、1: RemoteHost、 2: Header、 3: URL参数）\n- fieldName：Header名称/URL参数名称，仅在参数属性为2、3时填写\n- matchStrategy：属性值匹配方式，仅在参数属性为2、3且需要属性值匹配时填写（0: 精确、 2: 正则、 3: 子串）\n- pattern：匹配串，仅在参数属性为2、3且需要属性值匹配时填写",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "地域ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Resource",
      "description": "接口名称：适用该规则的应用资源。需要与控制台接口详情处接口名称一致。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ResourceMode",
      "description": "资源模式\n- 0(资源名模式)",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "ResourceType",
      "description": "资源类型\n- 0(自定义接口)\n- 1(WEB)\n- 2(RPC)",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "StatIntervalMs",
      "description": "统计窗口时长（单位毫秒）：统计的时间窗口长度，默认值为1秒。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Threshold",
      "description": "阈值",
      "position": "Query",
      "type": "Float",
      "required": true
    }
  ]
}