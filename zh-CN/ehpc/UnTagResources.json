{
  "name": "UnTagResources",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "All",
      "description": "是否解绑资源上全部的标签。当请求中未设置TagKey.N时，该参数才有效。取值范围：\n\n- true\n\n- false\n\n默认值：false。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "资源所属的地域ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceId",
      "description": "资源ID，最多50个。",
      "position": "Query",
      "type": "RepeatList",
      "required": true
    },
    {
      "name": "ResourceType",
      "description": "资源类型。目前仅支持cluster，即E-HPC集群。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "TagKey",
      "description": "资源的标签键，最多20个。",
      "position": "Query",
      "type": "RepeatList",
      "required": false
    }
  ]
}