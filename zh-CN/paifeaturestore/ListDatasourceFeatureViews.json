{
  "name": "ListDatasourceFeatureViews",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v1/instances/[InstanceId]/datasources/[DatasourceId]/featureviews",
  "parameters": [
    {
      "name": "All",
      "description": "是否返回所有数据（不分页）。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "DatasourceId",
      "description": "数据源ID，可通过接口ListDatasources获取。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "EndDate",
      "description": "查询读写量的截止时间，格式为yyyy-mm-dd。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "InstanceId",
      "description": "实例ID，可通过接口ListInstances获取。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "Name",
      "description": "特征视图名称模糊过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Order",
      "description": "排序方式。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "页码。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "页大小。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ProjectId",
      "description": "项目ID，可通过接口ListProjects获取。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ProjectName",
      "description": "项目名称过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ShowStorageUsage",
      "description": "是否展示存储用量，默认为true。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "SortBy",
      "description": "排序依据。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StartDate",
      "description": "查询读写量的起始时间，格式为yyyy-mm-dd。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Type",
      "description": "特征视图类型。\n- Batch-离线特征\n- Stream-实时特征\n- Sequence-序列特征",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Verbose",
      "description": "是否展示详细信息。如果设置为false，则不展示每个视图的UsageStatistics，只展示总量。默认为true。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}