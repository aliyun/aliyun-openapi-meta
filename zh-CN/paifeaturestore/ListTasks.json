{
  "name": "ListTasks",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v1/instances/[InstanceId]/tasks",
  "parameters": [
    {
      "name": "InstanceId",
      "description": "实例ID，可从接口ListInstances获取。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "ObjectId",
      "description": "任务对象ID，可从接口ListModelFeatures、ListFeatureViews获取。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ObjectType",
      "description": "任务对象类型。\n\n● ModelFeature-模型特征。\n\n● FeatureView-特征视图。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "页码，起始值为1，默认值为1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "页大小，默认为10。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ProjectId",
      "description": "项目ID，可从接口ListProjects获取。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Status",
      "description": "状态。\n\n● Initializing-初始化中。\n\n● Running-运行中。\n\n● Success-成功。\n\n● Failure-失败。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TaskIds",
      "description": "任务ID过滤。",
      "position": "Query",
      "type": "RepeatList",
      "required": false
    },
    {
      "name": "Type",
      "description": "任务类型过滤。\n● OfflineToOnline-离在线数据同步。\n● ExportTrainingSet-导出训练样本表。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}