{
  "name": "ListTopics",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/instances/[instanceId]/topics",
  "parameters": [
    {
      "name": "filter",
      "description": "查询的过滤条件，若不输入则查询该实例下所有的主题。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "instanceId",
      "description": "指定需要获取哪个实例下的主题列表。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "messageTypes",
      "description": "主题的消息类型。",
      "position": "Query",
      "type": "RepeatList",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "分页页码，查询第几页的返回结果。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "pageSize",
      "description": "分页大小，每页最多显示的返回结果数量。",
      "position": "Query",
      "type": "Integer",
      "required": true
    }
  ]
}