{
  "name": "ListMigrationOperations",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/migrations/[migrationId]/stages/[stageType]/operations",
  "parameters": [
    {
      "name": "filter",
      "description": "查询的过滤条件。\n\n>\n> - 支持基于operationKey，即Topic名称进行过滤",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "instanceId",
      "description": "实例ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "migrationId",
      "description": "迁移ID。",
      "position": "Path",
      "type": "Long",
      "required": true
    },
    {
      "name": "operationType",
      "description": "操作类型。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "pageNumber",
      "description": "分页页码，查询第几页的返回结果。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "pageSize",
      "description": "分页大小，每页最多显示的返回结果数量。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "stageType",
      "description": "迁移阶段类型。",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}