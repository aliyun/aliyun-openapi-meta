{
  "name": "ListTraces",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/instances/[instanceId]/topics/[topicName]/traces",
  "parameters": [
    {
      "name": "endTime",
      "description": "结束时间。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "instanceId",
      "description": "实例ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "liteTopicName",
      "description": "轻量主题名称",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "messageId",
      "description": "消息ID。\n\nqueryType选择MESSAGE_ID时必填。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "messageKey",
      "description": "消息key。\n\nqueryType选择MESSAGE_KEY时必填。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "分页页码，查询第几页的返回结果。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "pageSize",
      "description": "分页大小，每页做多显示的返回结果数量。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "queryType",
      "description": "查询类型。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "startTime",
      "description": "开始时间。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "topicName",
      "description": "主题名称。",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}