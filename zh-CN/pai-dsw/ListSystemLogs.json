{
  "name": "ListSystemLogs",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/systemlogs",
  "parameters": [
    {
      "name": "GmtEndTime",
      "description": "结束UTC时间，日期格式 iso8601。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "GmtStartTime",
      "description": "起始UTC时间，日期格式 iso8601。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "InstanceId",
      "description": "DSW实例Id。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "LifecycleId",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "LogLevel",
      "description": "日志等级，目前支持\n\n- Error",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "LogRepository",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Offset",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Order",
      "description": "排序方向：\n\n- ASC 升序\n- DESC 降序",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ProblemCategory",
      "description": "问题分类\n\n- InstanceStartFailed\n- InstanceTerminating\n- InstanceCreateFailed",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SortBy",
      "description": "排序字段：支持以下字段\n\n- GmtCreateTime\n- GmtModifiedTime",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SourceRequestId",
      "description": "出问题的请求RequestId。",
      "position": "Query",
      "type": "String",
      "required": false
    }
