{
  "name": "GetUserCommand",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/usercommands/[UserCommandId]",
  "parameters": [
    {
      "name": "InstanceId",
      "description": "DSW实例ID，填写此项用于查找与该实例绑定的UserCommand。传入该选项会覆盖UserCommandId的设置。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Token",
      "description": "使用GetToken接口签发的Token访问UserCommand。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "UserCommandId",
      "description": "用户自定义命令的ID。可通过GetInstance等接口获取。",
      "position": "Path",
      "type": "String",
      "required": false
    }
  ]
}