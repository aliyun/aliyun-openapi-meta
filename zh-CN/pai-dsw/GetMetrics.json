{
  "name": "GetMetrics",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/instance/[InstanceId]/cms/metrics",
  "parameters": [
    {
      "name": "Dimensions",
      "description": "（必填）请求参数。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "EndTime",
      "description": "截止时间，默认是当前。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "InstanceId",
      "description": "（必填）DSW 实例 ID。",
      "position": "Path",
      "type": "String",
      "required": false
    },
    {
      "name": "Length",
      "description": "每一页显示的最大记录数，不超过1440。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "MetricName",
      "description": "（必填）监控指标的名字。请参考 [指标名字](https://cms.console.aliyun.com/metric-meta/acs_pai_dsw/dsw)",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Namespace",
      "description": "（必填）指标保存的云监控的空间。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "NextToken",
      "description": "分页游标标识。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Period",
      "description": "时间间隔。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StartTime",
      "description": "起始时间，默认是1小时前。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}