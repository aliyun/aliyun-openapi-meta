{
  "name": "DescribeServiceLog",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/services/[ClusterId]/[ServiceName]/logs",
  "parameters": [
    {
      "name": "ClusterId",
      "description": "服务所在地域ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "ContainerName",
      "description": "服务实例的容器名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "EndTime",
      "description": "查询的结束UTC时间。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "InstanceName",
      "description": "服务实例的名称。如何获取实例名称，请参见[ListServiceInstances](~~412108~~)。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Ip",
      "description": "要查询的机器IP。如何获取机器IP，请参见[ListServiceInstances](~~412108~~)。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Keyword",
      "description": "查询的关键字。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNum",
      "description": "服务日志的页码，默认为1。",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页查询时设置的每页显示的日志行数，默认值为500。",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "Previous",
      "description": "查看实例上次重启前的日志。注意：仅在实例出现重启时可用。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "ServiceName",
      "description": "服务名称。如何获取服务名称，请参见[ListServices ](~~412109~~)。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "StartTime",
      "description": "查询的开始UTC时间。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}