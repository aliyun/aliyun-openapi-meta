{
  "name": "ListResourceInstanceWorker",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/resources/[ClusterId]/[ResourceId]/instance/[InstanceName]/workers",
