{
  "name": "PutLayerACL",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "PUT",
  "pathPattern": "/2021-04-06/layers/[layerName]/acl",
  "parameters": [
    {
      "name": "X-Fc-Account-Id",
      "description": "您的阿里云账号（主账号）ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Date",
      "description": "API调用时间，格式为**EEE,d MMM yyyy HH:mm:ss GMT**。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Trace-Id",
      "description": "请求的调用链ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "layerName",
      "description": "层的名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "public",
      "description": "是否公开。\n- **true**：公开。\n- **false**：不公开。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}