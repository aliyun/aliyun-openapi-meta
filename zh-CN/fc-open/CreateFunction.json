{
  "name": "CreateFunction",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "/2021-04-06/services/[serviceName]/functions",
  "parameters": [
    {
      "name": "X-Fc-Account-Id",
      "description": "您的阿里云账号（主账号）ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Code-Checksum",
      "description": "函数代码包的CRC-64值。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Date",
      "description": "函数的调用开始日期，格式为**EEE,d MMM yyyy HH:mm:ss GMT**。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Trace-Id",
      "description": "调用函数的ID，与返回参数requestId对应。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "function",
      "description": "函数的定义。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "serviceName",
      "description": "服务的名称。",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}