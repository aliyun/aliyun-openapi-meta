{
  "name": "RegisterEventSource",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "/2021-04-06/services/[serviceName]/functions/[functionName]/event-sources",
  "parameters": [
    {
      "name": "X-Fc-Account-Id",
      "description": "您的阿里云账号（主账号）ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Date",
      "description": "函数的调用开始日期，格式为**EEE,d MMM yyyy HH:mm:ss GMT**。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Trace-Id",
      "description": "函数计算调用请求的调用链ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "body",
      "description": "请求体参数。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "functionName",
      "description": "函数名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "qualifier",
      "description": "别名或版本。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "serviceName",
      "description": "服务名称。",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}