{
  "name": "GetLayerVersion",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/2021-04-06/layers/[layerName]/versions/[version]",
  "parameters": [
    {
      "name": "X-Fc-Account-Id",
      "description": "您的阿里云账号（主账号）ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Date",
      "description": "函数的调用开始日期，格式为**EEE,d MMM yyyy HH:mm:ss GMT**。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "X-Fc-Trace-Id",
      "description": "请求的调用链ID。",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "layerName",
      "description": "层的名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "version",
      "description": "层的版本。",
      "position": "Path",
      "type": "String",
      "required": true
    }
  ]
}