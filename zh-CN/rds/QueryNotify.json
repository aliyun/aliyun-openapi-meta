{
  "name": "QueryNotify",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "From",
      "description": "查询开始时间。格式：<i>yyyy-MM-dd</i>T<i>HH:mm:ss</i>Z（UTC时间）。",
      "position": "Body",
      "type": "String",
      "required": true
    },
    {
      "name": "PageNumber",
      "description": "页码，取值：大于**0**且不超过Integer的最大值。\n\n默认值：**1**。",
      "position": "Body",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "每页记录数，取值：\n* **30**\n* **50**\n* **100**\n\n默认值：**30**。",
      "position": "Body",
      "type": "Integer",
      "required": false
    },
    {
      "name": "To",
      "description": "查询结束时间，需要晚于开始时间。格式：<i>yyyy-MM-dd</i>T<i>HH:mm:ss</i>Z（UTC时间）。",
      "position": "Body",
      "type": "String",
      "required": true
    },
    {
      "name": "WithConfirmed",
      "description": "查询结果中是否包含已确认的通知。取值：\n- **true**：包含\n- **false**：不包含\n>已确认的通知即已通过ConfirmNotify接口标记为已确认的通知。",
      "position": "Body",
      "type": "Boolean",
      "required": true
    }
  ]
}