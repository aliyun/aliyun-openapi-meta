{
  "name": "CreateDBInstanceEndpoint",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "ClientToken",
      "description": "该参数用于保证请求的幂等性，防止重复提交请求。参数值由客户端生成，保证在不同请求间唯一。\n\n该参数值由ASCII字符组成，最长不超过64个字符，不能包含非ASCII字符。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ConnectionStringPrefix",
      "description": "内网连接地址前缀。\n\n创建Endpoint的同时会自动创建一个内网连接地址，该参数表示内网连接地址的前缀。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "DBInstanceEndpointDescription",
      "description": "用户自定义的Endpoint描述。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DBInstanceEndpointType",
      "description": "Endpoint类型，取值含义如下：\n\n- Primary：实例的读写Endpoint\n\n- Readonly：实例的只读Endpoint",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "DBInstanceId",
      "description": "实例ID。可调用DescribeDBInstances查询。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "NodeItems",
      "description": "Endpoint节点相关信息列表。",
      "position": "Query",
      "type": "Json",
      "required": true,
      "sub_parameters": [
        {
          "name": "DBInstanceId",
          "position": "Query",
          "type": "String",
          "required": true
        },
        {
          "name": "NodeId",
          "position": "Query",
          "type": "String",
          "required": true
        },
        {
          "name": "Weight",
          "position": "Query",
          "type": "Long",
          "required": false
        }
      ]
    },
    {
      "name": "Port",
      "description": "内网连接端口号。创建内网连接支持自定义端口号。\n\n取值范围：3000～5999",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "PrivateIpAddress",
      "description": "内网连接的IP地址。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "资源组ID。可调用DescribeDBInstanceAttribute获取。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "VSwitchId",
      "description": "内网连接的交换机ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "VpcId",
      "description": "内网连接的VPC ID。",
      "position": "Query",
      "type": "String",
      "required": true
    }
  ]
}