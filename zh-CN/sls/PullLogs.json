{
  "name": "PullLogs",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/logstores/[logStore]/shards/[shardId]?type=log",
  "parameters": [
    {
      "name": "Accept-Encoding",
      "description": "期望服务端返回的数据压缩格式，支持 lz4",
      "position": "Header",
      "type": "String",
      "required": false
    },
    {
      "name": "count",
      "description": "返回的Loggroup数目，最小值为1，最大值为1000。",
      "position": "Query",
      "type": "Integer",
      "required": true
    },
    {
      "name": "cursor",
      "description": "游标，表示从什么位置开始读取数据，相当于起点。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "end_cursor",
      "description": "结束游标，表示读取数据到什么地方结束，相当于终点。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "logStore",
      "description": "Logstore名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "project",
      "description": "Project名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "query",
      "description": "过滤语句，spl 语法，请参见[SPL指令](~~2536530~~)",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "shardId",
      "description": "日志库的分片 id。",
      "position": "Path",
      "type": "Integer",
      "required": true
    }
  ]
}