{
  "name": "ListStoreViews",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/storeviews",
  "parameters": [
    {
      "name": "name",
      "description": "用于模糊搜索的数据集名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "offset",
      "description": "返回结果在全部数据集中的偏移位置， 默认为 0 。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "project",
      "description": "日志项目名称。",
      "position": "Host",
      "type": "String",
      "required": true
    },
    {
      "name": "size",
      "description": "期望返回的数据集个数， 默认值为 100。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "storeType",
      "description": "数据集类型，默认不根据类型过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}