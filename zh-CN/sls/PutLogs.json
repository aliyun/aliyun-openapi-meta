{
  "name": "PutLogs",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "/logstores/[logstore]/shards/lb",
  "parameters": [
    {
      "name": "body",
      "description": "压缩后的 pb 日志数据",
      "position": "Body",
      "type": "Json",
      "required": false
    },
    {
      "name": "logstore",
      "description": "Logstore名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "project",
      "description": "Project名称。",
      "position": "Host",
      "type": "String",
      "required": true
    },
    {
      "name": "x-log-compresstype",
      "description": "日志压缩类型，目前支持lz4、gzip两种类型。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}