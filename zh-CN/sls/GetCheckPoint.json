{
  "name": "GetCheckPoint",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/logstores/[logstore]/consumergroups/[consumerGroup]",
  "parameters": [
    {
      "name": "consumerGroup",
      "description": "消费组名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "logstore",
      "description": "Logstore名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "project",
      "description": "Project 名称。",
      "position": "Host",
      "type": "String",
      "required": true
    },
    {
      "name": "shard",
      "description": "Shard ID。\n\n- 如果指定的Shard不存在，则返回空列表。\n- 如果不指定Shard，则返回所有Shard的Checkpoint。",
      "position": "Query",
      "type": "Integer",
      "required": false
    }
  ]
