{
  "name": "ListConfig",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/configs",
  "parameters": [
    {
      "name": "configName",
      "description": "Logtail配置名称，用于模糊搜索。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "logstoreName",
      "description": "Logstore名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "offset",
      "description": "查询开始行。默认值为0。",
      "position": "Query",
      "type": "Long",
      "required": true
    },
    {
      "name": "project",
      "description": "Project名称。",
      "position": "Host",
      "type": "String",
      "required": true
    },
    {
      "name": "size",
      "description": "分页查询时，设置的每页行数。最大值为500。",
      "position": "Query",
      "type": "Long",
      "required": true
    }
  ]
}