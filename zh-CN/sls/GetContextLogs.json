{
  "name": "GetContextLogs",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/logstores/[logstore]?type=context_log",
  "parameters": [
    {
      "name": "back_lines",
      "description": "指定起始日志往前（上文）的日志条数，取值范围为`(0,100]`。",
      "position": "Query",
      "type": "Long",
      "required": true
    },
    {
      "name": "forward_lines",
      "description": "指定起始日志往后（下文）的日志条数，取值范围为`(0,100]`。",
      "position": "Query",
      "type": "Long",
      "required": true
    },
    {
      "name": "logstore",
      "description": "Logstore名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "pack_id",
      "description": "起始日志所属的LogGroup的唯一身份标识。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "pack_meta",
      "description": "起始日志在对应LogGroup内的唯一上下文结构标识。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "project",
      "description": "Project名称。",
      "position": "Host",
      "type": "String",
      "required": true
    }
  ]
}