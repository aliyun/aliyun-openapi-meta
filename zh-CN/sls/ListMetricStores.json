{
  "name": "ListMetricStores",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/metricstores",
  "parameters": [
    {
      "name": "mode",
      "description": "要查询的时序库规格类型，例如 standard",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "name",
      "description": "要查询的时序库名称，支持模糊查询，此参数为空或者不传递此参数表示查询所有时序库",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "offset",
      "description": "查询分页的起始位置",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "project",
      "description": "日志项目名称",
      "position": "Host",
      "type": "String",
      "required": true
    },
    {
      "name": "size",
      "description": "查询分页的页大小，一次请求最多返回的时序库数量",
      "position": "Query",
      "type": "Integer",
      "required": false
    }
  ]
}