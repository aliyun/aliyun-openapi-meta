{
  "name": "GetAsyncSql",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/asyncsql/[queryId]",
  "parameters": [
    {
      "name": "line",
      "description": "分页结果的page大小，最大支持1000",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "offset",
      "description": "分页结果的offset",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "project",
      "description": "sls的project名称",
      "position": "Host",
      "type": "String",
      "required": false
    },
    {
      "name": "queryId",
      "description": "异步SQL对应的ID（整个SQL生命周期内固定）",
      "position": "Path",
      "type": "String",
      "required": false
    }
  ]
}