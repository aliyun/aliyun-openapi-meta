{
  "name": "AddCdnDomain",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "CdnType",
      "description": "加速域名的业务类型。取值：\n\n- **web**：图片小文件。\n- **download**：大文件下载。\n- **video**：视音频点播。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "CheckUrl",
      "description": "健康检测URL。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DomainName",
      "description": "需要接入CDN的加速域名。\n\n支持泛域名，以英文句号（.）开头，例如.example.com。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ResourceGroupId",
      "description": "资源组ID。\n\n不传该参数时，系统自动补全为默认资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Scope",
      "description": "加速区域。取值：\n\n- **domestic**（默认值）：仅中国内地。\n\n- **overseas**：全球（不包含中国内地）。\n\n- **global**：全球。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SecurityToken",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Sources",
      "description": "回源地址列表。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Tag",
      "description": "标签信息列表。最大可填入20个标签信息。",
      "position": "Query",
      "type": "RepeatList",
      "required": false,
      "sub_parameters": [
        {
          "name": "Key",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "Value",
          "position": "Query",
          "type": "String",
          "required": false
        }
      ]
    },
    {
      "name": "TopLevelDomain",
      "description": "顶级接入域。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}