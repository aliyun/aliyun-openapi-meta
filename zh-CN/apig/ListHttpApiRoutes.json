{
  "name": "ListHttpApiRoutes",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/v1/http-apis/[httpApiId]/routes",
  "parameters": [
    {
      "name": "consumerAuthorizationRuleId",
      "description": "根据特定的消费者授权规则ID过滤接口列表，响应中的接口列表仅包含已授权的接口。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "deployStatuses",
      "description": "路由的部署状态。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "domainId",
      "description": "根据域名ID过滤路由信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "environmentId",
      "description": "环境id。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "forDeploy",
      "description": "",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "gatewayId",
      "description": "云原生API网关ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "httpApiId",
      "description": "HTTP API ID。",
      "position": "Path",
      "type": "String",
      "required": false
    },
    {
      "name": "name",
      "description": "路由名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "nameLike",
      "description": "按路由名称模糊查询。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "分页页码，从1开始，不填默认1.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "分页大小，有效范围[1, 100]，不填默认10。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pathLike",
      "description": "按路由路径模糊查询。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withAuthPolicyInfo",
      "description": "响应中携带消费者授权信息。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "withConsumerInfoById",
      "description": "响应中每个路由信息携带指定消费者ID的授权规则列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withPluginAttachmentByPluginId",
      "description": "响应中每个路由信息携带指定的插件ID的挂载信息。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}