{
  "name": "ListPlugins",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/v1/plugins",
  "parameters": [
    {
      "name": "attachResourceId",
      "description": "挂载资源ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "attachResourceType",
      "description": "策略支持挂载点类型。\n\n- HttpApi：HttpApi。\n- Operation：HttpApi的Operation。\n- GatewayRoute：网关路由。\n- GatewayService：网关服务。\n- GatewayServicePort：网关服务端口。\n- Domain：网关域名。\n- Gateway：网关。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "gatewayId",
      "description": "网关实例id过滤参数",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "gatewayType",
      "description": "网关类型过滤，目前支持**AI**、**API**两种网关类型。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "includeBuiltinAiGateway",
      "description": "返回结果是否需要包含系统内置安装的AI插件，默认false",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "分页页码。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "分页大小",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pluginClassId",
      "description": "插件类型id过滤参数",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pluginClassName",
      "description": "插件类型名称过滤参数",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withAttachmentInfo",
      "description": "返回结果是否需要包含attachResourceId对应的插件挂载信息",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}