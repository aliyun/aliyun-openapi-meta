  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/v1/services",
  "parameters": [
    {
      "name": "gatewayId",
      "description": "云原生API网关ID。",
      "position": "Query",
      "type": "String",
