{
  "name": "ListPluginClasses",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/v1/plugin-classes",
  "parameters": [
    {
      "name": "aliasLike",
      "description": "插件别名，模糊搜索。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "direction",
      "description": "出入站方向：\n- 出站：OutBound。\n- 入栈：InBound。\n- 双方向：Both。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "excludeBuiltinAiProxy",
      "description": "是否排除内置的插件",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "gatewayId",
      "description": "网关id。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "gatewayType",
      "description": "网关类型过滤，目前支持**AI**、**API**两种网关类型。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "installed",
      "description": "是否已安装。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "nameLike",
      "description": "插件名称，模糊搜索。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "页码，默认1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "分页大小。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "source",
      "description": "插件来源：\n- Higress 官方：HigressOfficial\n- Higress 社区：HigressCommunity\n- 自定义：Custom",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "type",
      "description": "插件类型。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}