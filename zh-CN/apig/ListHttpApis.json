{
  "name": "ListHttpApis",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/v1/http-apis",
  "parameters": [
    {
      "name": "gatewayId",
      "description": "云原生API网关ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "gatewayType",
      "description": "网关类型过滤，目前支持**AI**、**API**两种网关类型。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "keyword",
      "description": "搜索关键词，支持API名称模糊搜索或API ID精确搜索。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "name",
      "description": "按照名称精确搜索。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "pageNumber",
      "description": "分页页码，从1开始，不填默认1.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "分页大小，有效范围[1, 100]，不填默认10。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "resourceGroupId",
      "description": "资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "types",
      "description": "HTTP API的类型。支持传入多个类型，采用\",\"分割。\n- Http\n- Rest\n- WebSocket\n- HttpIngress",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withAPIsPublishedToEnvironment",
      "description": "",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "withAuthPolicyInEnvironmentId",
      "description": "响应中每个API信息携带指定环境ID下的消费者认证策略信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withAuthPolicyList",
      "description": "是否开启了权限认证策略",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "withConsumerInfoById",
      "description": "响应中每个API信息携带指定消费者ID的授权规则列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withEnvironmentInfo",
      "description": "环境信息",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "withEnvironmentInfoById",
      "description": "环境ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withIngressInfo",
      "description": "是否包含ingress信息",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "withPluginAttachmentByPluginId",
      "description": "插件ID，根据此插件ID获取插件发布信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "withPolicyConfigs",
      "description": "是否包含策略信息",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}