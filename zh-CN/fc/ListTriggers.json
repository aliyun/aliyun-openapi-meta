{
  "name": "ListTriggers",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/2023-03-30/functions/[functionName]/triggers",
  "parameters": [
    {
      "name": "functionName",
      "description": "函数名称",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "limit",
      "description": "返回的触发器数量",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "nextToken",
      "description": "分页起始标记",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "prefix",
      "description": "触发器名称前缀",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}