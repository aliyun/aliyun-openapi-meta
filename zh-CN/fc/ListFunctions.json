{
  "name": "ListFunctions",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/2023-03-30/functions",
  "parameters": [
    {
      "name": "description",
      "description": "需要过滤的函数描述。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "fcVersion",
      "description": "函数所属的版本。\n- v3：只列出FC3.0的函数。\n- v2：只列出FC2.0的函数。\n\n默认不填，即列出FC3.0和FC2.0的函数。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "functionName",
      "description": "函数名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "gpuType",
      "description": "需要过滤的函数GPU卡型。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "limit",
      "description": "返回函数的数量，最小值为1，最大值为100。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "nextToken",
      "description": "分页起始标记。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "prefix",
      "description": "函数名称前缀。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "resourceGroupId",
      "description": "资源组 ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "runtime",
      "description": "需要过滤的函数运行时。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "tags",
      "description": "需要过滤的函数标签。",
      "position": "Query",
      "type": "Json",
      "required": false
    }
  ]
}