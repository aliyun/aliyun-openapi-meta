{
  "name": "PutLayerACL",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "PUT",
  "pathPattern": "/2023-03-30/layers/[layerName]/acl",
  "parameters": [
    {
      "name": "acl",
      "description": "设置层的访问权限，1：公开，0：私有，默认为私有",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "layerName",
      "description": "层名称",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "public",
      "description": "是否公开层。true或者false。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}