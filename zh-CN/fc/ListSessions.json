{
  "name": "ListSessions",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/2023-03-30/functions/[functionName]/sessions",
  "parameters": [
    {
      "name": "functionName",
      "description": "函数名",
      "position": "Path",
      "type": "String",
      "required": false
    },
    {
      "name": "limit",
      "description": "返回的会话数量，如不填写默认返回 20条。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "nextToken",
      "description": "分页起始标记",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "qualifier",
      "description": "函数别名或版本信息",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "sessionId",
      "description": "过滤的SessionId值，如指定，可返回此会话关联的所有 Active/Expired 状态信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "sessionStatus",
      "description": "需要过滤的会话状态，默认返回所有Active/Expired状态的会话信息，也可指定 Active 仅获取活跃的会话信息，或指定Expired仅获取过期的会话信息。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}