{
  "name": "ListHpoTrials",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/automl/v1/hpo/experiment/[ExperimentId]/trials",
  "parameters": [
    {
      "name": "ExperimentId",
      "description": "HPO实验id",
      "position": "Path",
      "type": "String",
      "required": false
    },
    {
      "name": "Order",
      "description": "分页查询时，对指定的排序字段进行升序或降序。默认ASC。\n- ASC\n升序\n- DESC\n降序",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "1-based 结果页面序号。做为搜索实验的限制条件。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "当前查询的页大小（每页上显示的trials的个数）",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "SortBy",
      "description": "排序依据",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}