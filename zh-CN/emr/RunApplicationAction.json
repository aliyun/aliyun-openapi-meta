{
  "name": "RunApplicationAction",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "ActionName",
      "description": "动作名称。取值范围：\n- start：启动。\n- stop：停止。\n- config：下发配置。\n- restart：重启 。\n- refresh_queues：刷新YARN队列。\n- refresh_labels：刷新YARN标签。\n- refresh_nodes:  刷新 YARN 集群中节点信息。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "BatchSize",
      "description": "每批次数量。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ClusterId",
      "description": "集群ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ComponentInstanceSelector",
      "description": "操作对象。",
      "position": "Query",
      "type": "Json",
      "required": true
    },
    {
      "name": "Description",
      "description": "描述。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ExecuteStrategy",
      "description": "运行策略。取值范围：\n- FAILED_BLOCK：失败后阻塞。\n- FAILED_CONTINUE：失败后继续。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Interval",
      "description": "滚动执行间隔时间。单位：秒。",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "地域ID。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "RollingExecute",
      "description": "是否滚动执行。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}