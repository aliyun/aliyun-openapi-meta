{
  "name": "UpdateIstioInjectionConfig",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "DataPlaneMode",
      "description": "命名空间的数据面模式，仅在当前服务网格实例启用Ambient Mesh模式的情况下有效。取值：\n\n- ambient：将命名空间的数据面模式设定为ambient模式。\n\n- sidecar：将命名空间的数据面模式设定为sidecar模式。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "EnableIstioInjection",
      "description": "是否启用自动注入。",
      "position": "Body",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "EnableSidecarSetInjection",
      "description": "是否启用SidecarSet注入。",
      "position": "Body",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "IstioRev",
      "description": "为命名空间指定的注入版本，仅在服务网格进行金丝雀升级的过程中有效。当IstioRev不为空时，不能同时传入EnableIstioInjection或EnableSidecarSetInjection。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "Namespace",
      "description": "命名空间。",
      "position": "Body",
      "type": "String",
      "required": true
    },
    {
      "name": "ServiceMeshId",
      "description": "服务网格ID。",
      "position": "Body",
      "type": "String",
      "required": true
    }
  ]
}