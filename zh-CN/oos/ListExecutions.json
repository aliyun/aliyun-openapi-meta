{
  "name": "ListExecutions",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "Categories",
      "description": "执行的模板类型列表。可分为Other、TimerTrigger、EventTrigger、AlarmTrigger。此参数和Category参数只能同时传入一个，推荐使用Categories。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Category",
      "description": "执行的模版类型。可分为Other、TimerTrigger、EventTrigger、AlarmTrigger。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Depth",
      "description": "执行的深度，可分为RootDepth、FirstChildDepth\nRootDepth只返回主执行，FirstChildDepth只返回第一层的子执行。此参数和IncludeChildExecution参数只能同时传入一个，推荐使用Depth。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Description",
      "description": "执行的描述。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "EndDateAfter",
      "description": "结束时间大于等于指定时间的执行",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "EndDateBefore",
      "description": "结束时间小于等于指定时间的执行",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ExecutedBy",
      "description": "执行者",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ExecutionId",
      "description": "执行ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "IncludeChildExecution",
      "description": "是否包含子执行。默认为False。",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "MaxResults",
      "description": "分页大小。取值范围：10-100。默认为50。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "Mode",
      "description": "执行模式。取值范围：\n- **Automatic**\n- **Debug**",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "NextToken",
      "description": "翻页标记的Token",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ParentExecutionId",
      "description": "父执行ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "RamRole",
      "description": "RAM角色",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "RegionId",
      "description": "地域ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceId",
      "description": "ECS资源ID",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceTemplateName",
      "description": "资源的模板名称",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SortField",
      "description": "排序字段。取值范围：\n\n- **StartDate**（默认值）：创建时间\n- **EndDate**：结束时间\n- **Status**：状态",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SortOrder",
      "description": "排序方向。取值范围：\n- **Ascending**：升序\n- **Descending**（默认值）：降序",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StartDateAfter",
      "description": "开始时间大于等于指定时间的执行",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StartDateBefore",
      "description": "开始时间小于等于指定时间的执行",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Status",
      "description": "执行状态，取值范围：Running, Started, Success, Failed, Waiting, Cancelled, Pending, Skipped。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Tags",
      "description": "执行的标签",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TemplateName",
      "description": "模板名称。名称未包含关系，最终列出包含所填模板名称的所有模板。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}