{
  "name": "ListChangeRequests",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/appstack/apps/[appName]/changeRequests",
  "parameters": [
    {
      "name": "appName",
      "description": "应用名",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "appNameList",
      "description": "按应用名称进行搜索",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "displayNameKeyword",
      "description": "按展示名进行模糊搜索的关键字",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "nextToken",
      "description": "键集分页 token，获取第一页数据时无需传入，否则需要传入前一页查询结果中的 nextToken 字段",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "orderBy",
      "description": "分页排序属性，决定根据何种属性进行记录排序；推荐在实现严格遍历时，使用 id 属性",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "organizationId",
      "description": "企业标识，也称企业id",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ownerIdList",
      "description": "按ownerId进行搜索",
      "position": "Query",
      "type": "Json",
      "required": false
    },
    {
      "name": "page",
      "description": "页面分页时使用，用于获取下一页内容",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pagination",
      "description": "分页模式参数，目前只支持键集分页 keyset 模式",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "perPage",
      "description": "分页尺寸参数，决定一页最多返回多少对象",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "sort",
      "description": "分页排序升降序，asc 为升序，desc 为降序；推荐在实现严格遍历时，使用升序",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "stateList",
      "description": "按状态进行搜索",
      "position": "Query",
      "type": "Json",
      "required": false
    }
  ]
}