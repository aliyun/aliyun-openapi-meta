{
  "name": "ListAppReleaseStageExecutions",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/appstack/apps/[appName]/releaseWorkflows/[releaseWorkflowSn]/releaseStages/[releaseStageSn]/executions",
  "parameters": [
    {
      "name": "appName",
      "description": "应用名",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "nextToken",
      "description": "键集分页 token，获取第一页数据时无需传入，否则需要传入前一页查询结果中的 nextToken 字段",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "orderBy",
      "description": "分页排序属性，决定根据何种属性进行记录排序；默认 gmtCreate",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "organizationId",
      "description": "企业标识，也称企业id",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "page",
      "description": "键集分页模式下不需要传入",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pagination",
      "description": "分页模式参数：keyset表示键集分页",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "perPage",
      "description": "分页尺寸参数，决定一页最多返回多少对象，默认20，限制[1,100]",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "releaseStageSn",
      "description": "研发阶段sn",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "releaseWorkflowSn",
      "description": "研发流程sn",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "sort",
      "description": "分页排序升降序，asc 为升序，desc 为降序；默认 desc",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}