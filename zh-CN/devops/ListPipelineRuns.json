{
  "name": "ListPipelineRuns",
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/organization/[organizationId]/pipelines/[pipelineId]/pipelineRuns",
  "parameters": [
    {
      "name": "endTime",
      "description": "结束时间",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "maxResults",
      "description": "最大返回数量",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "nextToken",
      "description": "分页token",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "organizationId",
      "description": "企业标识，也称企业id，字符串形式，可在云效访问链接中获取，如https://devops.aliyun.com/organization/【OrganizationId】",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "pipelineId",
      "description": "流水线ID，可在流水线链接中获取，如https://flow.aliyun.com/pipelines/【PipelineId】/current",
      "position": "Path",
      "type": "Long",
      "required": true
    },
    {
      "name": "startTime",
      "description": "开始时间",
      "position": "Query",
      "type": "Long",
      "required": false
    },
    {
      "name": "status",
      "description": "状态 FAIL 运行失败 SUCCESS 运行成功 RUNNING 运行中",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "triggerMode",
      "description": "- 触发模式\n \n  1人工触发\n   \n  2定时触发 \n\n  3代码提交触发\n \n  5流水线触发\n\n  6WEBHOOK触发",
      "position": "Query",
      "type": "Integer",
      "required": false
    }
  ]
}