{
  "name": "AddDcdnDomain",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "CheckUrl",
      "description": "健康检测URL。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DomainName",
      "description": "待添加的加速域名，仅支持添加单个域名。\n\n支持添加泛域名，以点号（.）开头，例如.example.com。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "FunctionType",
      "description": "计算服务类型。取值：\n- **routine**：程序。\n- **image**：镜像。\n- **cloudFunction**：函数。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "资源组ID。不传该参数时，系统自动补全默认资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Scene",
      "description": "加速场景。支持：\n- **apiscene**：API加速。\n- **webservicescene**：网站业务加速。\n- **staticscene**：视频、图文类加速。\n- **（空）**：没有场景。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Scope",
      "description": "加速区域。取值：\n\n- **domestic**：仅中国内地。\n- **overseas**：全球（不包含中国内地）。\n- **global**：全球。\n\n默认取值为**domestic**。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Sources",
      "description": "回源地址列表。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Tag",
      "description": "标签信息。最多20个标签。",
      "position": "Query",
      "type": "RepeatList",
      "required": false,
      "sub_parameters": [
        {
          "name": "Key",
          "position": "Query",
          "type": "String",
          "required": false
        },
        {
          "name": "Value",
          "position": "Query",
          "type": "String",
          "required": false
        }
      ]
    },
    {
      "name": "TopLevelDomain",
      "description": "顶级接入域。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}