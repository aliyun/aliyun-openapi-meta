{
  "name": "AddDcdnIpaDomain",
  "protocol": "HTTP|HTTPS",
  "method": "GET|POST",
  "pathPattern": "",
  "parameters": [
    {
      "name": "CheckUrl",
      "description": "检测URL。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "DomainName",
      "description": "需要接入全站加速IPA的域名。\n\n支持泛域名，以英文句号（.）开头，例如：.example.com。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "Protocol",
      "description": "协议名称。取值：\n\n- **udp**：UDP协议。\n- **tcp**：TCP协议。\n\n\n> 例如：`{\"protocol\":\"udp\"}`。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ResourceGroupId",
      "description": "资源组ID。不传该参数时，系统自动补全默认资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Scope",
      "description": "加速区域。取值：\n\n- **domestic**：仅中国内地。\n\n- **overseas**：全球（不包含中国内地）。\n\n- **global**：全球。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SecurityToken",
      "description": "",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Sources",
      "description": "回源地址列表。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "TopLevelDomain",
      "description": "顶级接入域。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}