{
  "name": "ListLogstashPlugins",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/openapi/logstashes/[InstanceId]/plugins",
  "parameters": [
    {
      "name": "InstanceId",
      "description": "实例ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "name",
      "description": "插件名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "page",
      "description": "插件列表的分页数。默认值：1，最小值：1，最大值：200。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "size",
      "description": "分页查询时设置的每页条数。最小值：1，最大值：200。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "source",
      "description": "插件来源。可选值：\n\n- USER：自定义插件\n- SYSTEM：系统预置插件",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}