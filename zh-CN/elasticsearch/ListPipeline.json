{
  "name": "ListPipeline",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/openapi/logstashes/[InstanceId]/pipelines",
  "parameters": [
    {
      "name": "InstanceId",
      "description": "Logstash实例ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "page",
      "description": "分页查询时，设置当前页面的页码。取值范围：1~200，默认值：1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pipelineId",
      "description": "管道ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "size",
      "description": "分页查询时，设置每页包含管道的数量。取值范围：1~200。",
      "position": "Query",
      "type": "Integer",
      "required": false
    }
  ]
}