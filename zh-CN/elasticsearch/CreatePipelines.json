{
  "name": "CreatePipelines",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "/openapi/logstashes/[InstanceId]/pipelines",
  "parameters": [
    {
      "name": "ClientToken",
      "description": "用于保证请求的幂等性。由客户端生成该参数值，要保证在不同请求间唯一，最大不超过64个ASCII字符。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "InstanceId",
      "description": "Logstash实例ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "body",
      "description": "请求体参数，用来指定管道信息，更多详细信息请参见[logstash.yml](https://www.elastic.co/guide/en/logstash/6.7/logstash-settings-file.html)。",
      "position": "Body",
      "type": "Json",
      "required": false,
      "sub_parameters": [
        {
          "name": "batchDelay",
          "position": "Body",
          "type": "Integer",
          "required": false
        },
        {
          "name": "batchSize",
          "position": "Body",
          "type": "Integer",
          "required": false
        },
        {
          "name": "config",
          "position": "Body",
          "type": "String",
          "required": true
        },
        {
          "name": "description",
          "position": "Body",
          "type": "String",
          "required": false
        },
        {
          "name": "pipelineId",
          "position": "Body",
          "type": "String",
          "required": true
        },
        {
          "name": "queueCheckPointWrites",
          "position": "Body",
          "type": "Integer",
          "required": false
        },
        {
          "name": "queueMaxBytes",
          "position": "Body",
          "type": "Integer",
          "required": false
        },
        {
          "name": "queueType",
          "position": "Body",
          "type": "String",
          "required": false
        },
        {
          "name": "workers",
          "position": "Body",
          "type": "Integer",
          "required": false
        }
      ]
    },
    {
      "name": "trigger",
      "description": "是否保存并部署管道。可选值：\n\n- true：保存并部署\n- false（默认）：仅保存",
      "position": "Query",
      "type": "Boolean",
      "required": false
