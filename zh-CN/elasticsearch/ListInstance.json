{
  "name": "ListInstance",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/openapi/instances",
  "parameters": [
    {
      "name": "description",
      "description": "实例名称，支持模糊查询。例如搜索**abc**的所有实例，则可能返回**abc**、**abcde**、**xyabc**、**xabcy**的所有实例。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "esVersion",
      "description": "实例版本。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "instanceCategory",
      "description": "实例版本类型。可选值：\n\n- x-pack：商业版\n\n- advanced/IS：增强版\n\n- community：基础版",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "instanceId",
      "description": "实例ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "page",
      "description": "实例列表的页码。\n起始值：**1**，默认值：**1**。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "paymentType",
      "description": "实例的付费类型。可选值：\n\n- postpaid：按量付费\n\n- prepaid：包年包月",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "resourceGroupId",
      "description": "实例所在的资源组ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "size",
      "description": "分页查询时设置的每页条数。最大值：**100**，默认值：**10**。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "tags",
      "description": "实例标签。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "vpcId",
      "description": "实例所在的专有网络ID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "zoneId",
      "description": "实例所在的可用区ID。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}