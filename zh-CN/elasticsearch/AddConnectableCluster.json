{
  "name": "AddConnectableCluster",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "POST",
  "pathPattern": "/openapi/instances/[InstanceId]/connected-clusters",
  "parameters": [
    {
      "name": "InstanceId",
      "description": "当前实例ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "body",
      "description": "远程实例ID。用来指定需要进行网络互通的远程实例ID，要求与当前实例在同一专有网络下。",
      "position": "Body",
      "type": "String",
      "required": false
    },
    {
      "name": "clientToken",
      "description": "用于保证请求的幂等性。由客户端生成该参数值，要保证在不同请求间唯一，最大不超过64个ASCII字符。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}