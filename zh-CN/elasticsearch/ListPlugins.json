{
  "name": "ListPlugins",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/openapi/instances/[InstanceId]/plugins",
  "parameters": [
    {
      "name": "InstanceId",
      "description": "实例ID。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "name",
      "description": "插件名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "page",
      "description": "分页数。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "size",
      "description": "每页记录数。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "source",
      "description": "插件来源类型，仅支持SYSTEM（系统插件）。",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}