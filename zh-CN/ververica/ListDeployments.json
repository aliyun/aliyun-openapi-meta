{
  "name": "ListDeployments",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/deployments",
  "parameters": [
    {
      "name": "creator",
      "description": "创建者UID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "executionMode",
      "description": "deployment执行模式。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "labelKey",
      "description": "标签名。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "labelValueArray",
      "description": "标签值，多个值使用分号隔开。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "modifier",
      "description": "修改者UID。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "name",
      "description": "deployment名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "namespace",
      "description": "项目空间名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "pageIndex",
      "description": "分页参数，页码索引，表示所请求页码，最小值为1，默认值为1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "分页参数，表示所请求页码元素数量，最大值为100，最小值为1，默认值为10。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "sortName",
      "description": "排序方式，支持按创建时间和修改时间倒序返回数据。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "status",
      "description": "deployment最新作业状态。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "workspace",
      "description": "工作空间ID。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}