{
  "name": "RegisterCustomConnector",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "/api/v2/namespaces/[namespace]/connectors:register",
  "parameters": [
    {
      "name": "jarUrl",
      "description": "自定义连接器Jar包存放的地址，需要平台可访问该地址。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "namespace",
      "description": "项目空间名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "description": "工作空间ID。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}