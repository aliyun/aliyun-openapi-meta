{
  "name": "GetFolder",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/folder",
  "parameters": [
    {
      "name": "folderId",
      "description": "文件夹ID，为空返回根文件夹。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "namespace",
      "description": "项目空间名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "description": "工作空间ID。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}