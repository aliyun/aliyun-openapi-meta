{
  "name": "RegisterUdfFunction",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "/api/v2/namespaces/[namespace]/udfartifacts/function:registerUdfFunction",
  "parameters": [
    {
      "name": "className",
      "description": "自定义函数对应的类名。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "functionName",
      "description": "函数名称，一般与类名称相同，您可以先选择自定义其他名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "namespace",
      "description": "项目空间名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "udfArtifactName",
      "description": "自定义函数对应的Jar或者Python文件的名称。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "description": "工作空间ID。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}