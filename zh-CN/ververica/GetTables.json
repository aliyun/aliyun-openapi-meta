{
  "name": "GetTables",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/catalogs/[catalogName]/databases/[databaseName]/tables",
  "parameters": [
    {
      "name": "catalogName",
      "description": "catalog名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "databaseName",
      "description": "database名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "namespace",
      "description": "项目空间名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "tableName",
      "description": "表名，表名为空返回所有表信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "workspace",
      "description": "工作空间ID。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}