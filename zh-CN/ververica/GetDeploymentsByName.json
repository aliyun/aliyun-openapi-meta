{
  "name": "GetDeploymentsByName",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/deployments/name/[deploymentName]",
  "parameters": [
    {
      "name": "deploymentName",
      "description": "已部署作业名称",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "ignoreJobSummary",
      "description": "是否忽略返回结果中的作业摘要信息（如 jobName、status），仅返回 JobId，用于提升性能",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "ignoreResourceSetting",
      "description": "是否忽略资源配置信息（如 parallelism、CU 数量），减少返回数据体积",
      "position": "Query",
      "type": "Boolean",
      "required": false
    },
    {
      "name": "namespace",
      "description": "项目空间名称",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "description": "工作空间ID",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}