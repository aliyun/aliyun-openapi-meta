{
  "name": "GetDatabases",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/catalogs/[catalogName]/databases",
  "parameters": [
    {
      "name": "catalogName",
      "description": "catalog名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "databaseName",
      "description": "database名称，为空时返回所有database信息。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "namespace",
      "description": "项目空间名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "description": "工作空间ID。",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}