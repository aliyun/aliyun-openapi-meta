{
  "name": "ListDateSourceGenerations",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/openapi/ha3/instances/[instanceId]/data-sources/[dataSourceName]/generations",
  "parameters": [
    {
      "name": "dataSourceName",
      "description": "数据源名称。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "domainName",
      "description": "数据源部署机房",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "instanceId",
      "description": "实例id。",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "validStatus",
      "description": "有效状态（默认为true）\n\n1. true 返回Task执行完成且未过期的generation\n2. false 返回所有generation",
      "position": "Query",
      "type": "Boolean",
      "required": false
    }
  ]
}