{
  "name": "ListMessageMetrics",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/messages/metrics",
  "parameters": [
    {
      "name": "EndDate",
      "description": "结束日期，格式20220102。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "GroupId",
      "description": "关联人群Id。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "分页数，从1开始，默认为1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页大小，默认为10。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "ScheduleId",
      "description": "关联触达计划Id。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Signature",
      "description": "签名名称。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SignatureId",
      "description": "签名Id，同时只能指定签名名称或签名Id其中之一。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "StartDate",
      "description": "开始日期，格式20220102。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "TemplateCode",
      "description": "模板号。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TemplateId",
      "description": "模板Id，同时只能指定模板Code或模板Id其中之一。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TemplateType",
      "description": "模板类型。\n- 0 : 验证码。\n- 1 : 短信通知。\n- 2 : 推广短信。",
      "position": "Query",
      "type": "Integer",
      "required": false
    }
  ]
}