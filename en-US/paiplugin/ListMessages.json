{
  "name": "ListMessages",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/messages",
  "parameters": [
    {
      "name": "Datetime",
      "description": "发送日期，格式为20220101。",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "ErrorCode",
      "description": "短信错误码过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "GroupId",
      "description": "关联人群Id过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "MessageId",
      "description": "短信Id过滤，短信Id为SendMessage成功返回的Id。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "PageNumber",
      "description": "分页数，从1开始，默认为1。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PageSize",
      "description": "分页大小，默认为10。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "PhoneNumber",
      "description": "手机号码过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "RequestId",
      "description": "短信批处理Id过滤，短信批处理Id为SendMessage成功返回的RequestId。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "ScheduleId",
      "description": "关联触达计划Id过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Signature",
      "description": "签名名称过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "SignatureId",
      "description": "签名Id过滤，同时只能指定签名名称或签名Id其中之一。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "Status",
      "description": "短信发送状态过滤。\n- 0 : 发送中。\n- 1 : 发送成功。\n- 2 : 发送失败。",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "TemplateCode",
      "description": "模板号过滤。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TemplateId",
      "description": "模板Id过滤，同时只能指定模板Code或模板Id其中之一。",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "TemplateType",
      "description": "模板类型过滤。\n- 0 : 验证码。\n- 1 : 短信通知。\n- 2 : 推广短信。",
      "position": "Query",
      "type": "Integer",
      "required": false
    }
  ]
}