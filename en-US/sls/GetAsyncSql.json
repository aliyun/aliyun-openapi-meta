      "type": "Integer",
      "required": false
    },
    {
      "name": "offset",
      "description": "The offset for paginated results.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "project",
      "description": "The name of the project.",
      "position": "Host",
      "type": "String",
      "required": false
    },
    {
      "name": "queryId",
