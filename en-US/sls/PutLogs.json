{
  "name": "PutLogs",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "/logstores/[logstore]/shards/lb",
  "parameters": [
    {
      "name": "body",
      "description": "The compressed log data in protobuf format.",
      "position": "Body",
      "type": "Json",
      "required": false
    },
    {
      "name": "logstore",
      "description": "The name of the logstore.",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "project",
      "description": "The name of the project.",
      "position": "Host",
      "type": "String",
      "required": true
    },
    {
      "name": "x-log-compresstype",
      "description": "The compression type of the logs. Valid values: lz4 and gzip.",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}