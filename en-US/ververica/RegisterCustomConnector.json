{
  "name": "RegisterCustomConnector",
  "deprecated": false,
  "protocol": "HTTPS",
  "method": "POST",
  "pathPattern": "/api/v2/namespaces/[namespace]/connectors:register",
  "parameters": [
    {
      "name": "jarUrl",
      "description": "The URL in which the JAR package of the custom connector is stored. The platform must be able to access this address.",
      "position": "Query",
      "type": "String",
      "required": true
    },
    {
      "name": "namespace",
      "description": "The name of the namespace.",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "workspace",
      "description": "The workspace ID.",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}