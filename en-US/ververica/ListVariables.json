{
  "name": "ListVariables",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/api/v2/namespaces/[namespace]/variables",
  "parameters": [
    {
      "name": "namespace",
      "description": "The name of the namespace.",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "pageIndex",
      "description": "The page number. Minimum value: 1. Default value: 1.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "The number of entries per page. Valid values: 1 to 100. Default value: 10.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "workspace",
      "description": "The workspace ID.",
      "position": "Header",
      "type": "String",
      "required": true
    }
  ]
}