{
  "name": "ListInstanceAccount",
  "deprecated": false,
  "protocol": "HTTP|HTTPS",
  "method": "GET",
  "pathPattern": "/instances/[instanceId]/accounts",
  "parameters": [
    {
      "name": "accountStatus",
      "description": "The status of the account.\n\nValid values:\n\n*   DISABLE\n*   ENABLE",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "accountType",
      "description": "The account type.\n  - CUSTOMER\n  - DEFAULT",
      "position": "Query",
      "type": "String",
      "required": false
    },
    {
      "name": "instanceId",
      "description": "The instance ID.",
      "position": "Path",
      "type": "String",
      "required": true
    },
    {
      "name": "pageNumber",
      "description": "The page number. Default value: 1.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "pageSize",
      "description": "The number of entries per page. Default value: 10.",
      "position": "Query",
      "type": "Integer",
      "required": false
    },
    {
      "name": "username",
      "description": "The username of the account.",
      "position": "Query",
      "type": "String",
      "required": false
    }
  ]
}